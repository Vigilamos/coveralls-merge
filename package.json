{
  "name": "@sourceallies/cover-alls",
  "version": "1.0.2",
  "description": "All in one tool for managing coverage reports for Coveralls",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src -d lib",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha --report lcovonly",
    "eslint": "node node_modules/eslint/bin/eslint.js src/** test/unit/**",
    "prepublish": "npm run verify && npm run build",
    "test": "./node_modules/.bin/mocha",
    "verify": "npm run eslint && npm run test && echo Verify Passed!"
  },
  "keywords": [
    "coveralls"
  ],
  "author": "Source Allies",
  "contributors": [
    "Michael Parker <mparker@sourceallies.com>"
  ],
  "license": "APLv2",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chance": "^1.0.4",
    "eslint": "^3.3.1",
    "isparta": "^4.0.0",
    "lodash": "^4.15.0",
    "mocha": "^3.0.0",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "request": "^2.74.0",
    "xml2js": "^0.4.17"
  }
}
